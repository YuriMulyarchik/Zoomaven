<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="PositiveTestSuite">
    <test name="Positive tests" parallel="true" thread-count="4">
        <groups>
            <run>
                <include name="positive" />
            </run>
        </groups>
        <classes>
            <class name="AnimalsTest.AnimalsTest" />
            <class name="AnimalsTest.BirdsTest" />
            <class name="AnimalsTest.FishTest" />
            <class name="AnimalsTest.MammalsTest" />
            <class name="MenusTest.MenusTest" />
        </classes>
    </test>
</suite>